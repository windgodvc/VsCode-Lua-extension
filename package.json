{
	"name": "luaf",
    "displayName": "luaFunction",
    "description": "lua 函数列表显示",
	"version": "0.0.1",
	"publisher": "windgod",
	"icon": "icon.svg",
	"repository": {
		"type": "git",
		"url": "https://github.com/windgodvc/VsCode-Lua-extension"
	},
	"bugs": {
		"url": "https://github.com/windgodvc/VsCode-Lua-extension/issues"
	},
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"contributes": {
		"commands": [
			{
				"command": "extension.searchLuaFun",
				"title": "Search Lua Function"
			}
		]
	},
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.1.4",
		"vscode": "^1.0.0",
		"mocha": "^2.3.3",
		"@types/node": "^6.0.40"
	}
}
